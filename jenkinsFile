pipeline {
    agent any
    stages {
        stage('Checkout SCM') {
            steps {
                script {
                    git branch: 'main', credentialsId: 'github-token', url: 'https://github.com/GAbriElaJnbg30/Testing_ApiRest.git'
                }
            }
        }
        // Otras etapas...
    }
    post {
        always {
            script {
                try {
                    sh 'docker stop apirestgo-api-go-container'
                    sh 'docker rm apirestgo-api-go-container'
                } catch (Exception e) {
                    echo 'Docker container not found or already stopped.'
                }
            }
        }
    }
}
